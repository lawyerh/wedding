<div class="container">
    <div class="row">
        <div class="col-9">
            <h2>Welcome to the Wedding Planner</h2>
        </div>
        <div class="col-3">
            <a class="btn btn-secondary" href="/">Log Out</a>
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Wedding</th>
                        <th>Date</th>
                        <th>Guest</th>
                        <th>Actions</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach(var wedding in ViewBag.weddings)
                    {
                        <tr>
                            <td>wedding[""]</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>